# Environment Variables for Equity Analyzer
# Copy this file to .env and add your actual API keys
# DO NOT commit .env to version control

# Alpha Vantage API Key (Get free key at: https://www.alphavantage.co/support/#api-key)
ALPHA_VANTAGE_API_KEY=demo

# Finnhub API Key (Get free key at: https://finnhub.io/)
FINNHUB_API_KEY=your_finnhub_key_here

# Polygon.io API Key (Get free key at: https://polygon.io/)
POLYGON_API_KEY=your_polygon_key_here

# Application settings
APP_PORT=7070
ENABLE_DEBUG_MODE=true
